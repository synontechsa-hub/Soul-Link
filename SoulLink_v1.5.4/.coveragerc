[run]
branch = True
source = backend/app
omit =
    */migrations/*
    */tests/*
    */__init__.py
    backend/app/main.py
    _dev/*

[report]
show_missing = True
exclude_lines =
    pragma: no cover
    def __repr__
    if self.debug:
    if __name__ == .__main__.:
    raise AssertionError
    raise NotImplementedError
    if TYPE_CHECKING:
    pass
